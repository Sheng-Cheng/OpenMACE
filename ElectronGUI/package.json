{
  "name": "nasa-gui",
  "productName": "NASA_Map",
  "version": "1.0.0",
  "description": "NASA GUI for demo",
  "main": "main.js",
  "config": {
    "port": "8082"
  },
  "scripts": {
    "version": "tsc -v",
    "start": "cross-env NODE_ENV=development PORT=$npm_package_config_port electron .",
    "build": "./node_modules/.bin/webpack",
    "watch": "cross-env PORT=$npm_package_config_port ./node_modules/.bin/webpack-dev-server --hot --inline",
    "build:prod": "cross-env PORT=$npm_package_config_port webpack --env.prod -p",
    "prepare": "node ./node_modules/patch-package/index.js",
    "launch": "cross-env PORT=$npm_package_config_port parallel \"watch --nasa-gui:port=$npm_package_config_port\" \"start --nasa-gui:port=$npm_package_config_port\"",
    "build:launch": "cross-env PORT=$npm_package_config_port parallel \"build:prod --nasa-gui:port=$npm_package_config_port\" \"launch --nasa-gui:port=$npm_package_config_port\"",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish"
  },
  "author": "Scotch",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.5.4",
    "@turf/turf": "4.7.3",
    "babel-loader": "8.0.6",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-react": "7.0.0",
    "babelify": "10.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "0.28.5",
    "deepcopy": "0.6.3",
    "electron-prebuilt": "1.4.13",
    "electron-reload": "1.2.2",
    "file-loader": "1.1.5",
    "heatmap.js": "2.0.5",
    "leaflet": "1.5.1",
    "leaflet-draw": "1.0.4",
    "leaflet-geometryutil": "0.9.1",
    "material-ui": "0.20.2",
    "net": "1.0.2",
    "react": "16.8.6",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.8.6",
    "react-leaflet": "2.4.0",
    "react-leaflet-draw": "0.19.0",
    "react-leaflet-heatmap-layer": "1.0.4",
    "react-notification-system": "0.2.17",
    "react-tap-event-plugin": "3.0.3",
    "style-loader": "0.18.2",
    "ts-loader": "6.0.4",
    "typescript": "^2.9.2",
    "url-loader": "0.5.9",
    "webpack": "4.35.3"
  },
  "devDependencies": {
    "@types/leaflet": "1.4.4",
    "@types/material-ui": "0.21.6",
    "@types/node": "12.6.2",
    "@types/react": "16.8.23",
    "@types/react-bootstrap": "0.32.19",
    "@types/react-dom": "16.8.4",
    "@types/react-leaflet": "2.2.1",
    "@types/react-notification-system": "0.2.39",
    "browserify": "13.1.0",
    "electron-rebuild": "1.6.0",
    "npm-parallel": "^2.0.1",
    "patch-package": "^5.0.0",
    "prettier": "^1.14.2",
    "react-addons-perf": "15.4.2",
    "tsify": "3.0.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.9.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": ">=3.7.2"
  }
}
